with Colonies;
with Commodities;
with Employers;
with Managed;
with Network;
with Production;
with Transport;
with Utility;
with Worlds;

package Pops is

record Pop is
   key Faction;
   key Colony;
   key World;
   key World_Sector;
   Size   : Long_Float;
   Apathy : Long_Float;
   Wealth_Group : Pop_Group;
   key Faction_World with Faction, World;
end Pop;

record Pop_Group_Member is
   key Pop;
   Pop_Group;
end Pop_Group_Member;

end Pops;
